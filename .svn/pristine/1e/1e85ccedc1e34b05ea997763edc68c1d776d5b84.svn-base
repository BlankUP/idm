html,
body {
  height: 100%;
  text-align: left;
  font-family: "Helvetica Neue", Helvetica, "Microsoft YaHei", Arial, "宋体", sans-serif;
}
table.wholePage {
  width: 100%;
  height: 100%;
  padding: 0;
}
table.wholePage tr {
  padding: 0;
}
table.wholePage td {
  padding: 0;
  vertical-align: top;
}
table.wholePage td.main-part {
  height: 100%;
}
table.wholePage td.main-part table {
  width: 100%;
  height: 100%;
}
table.wholePage td.main-part table td.left-nav {
  background-color: white;
  width: 210px;
  height: 100%;
  border-right: 1px solid #dfdfdf;
  color: #999999;
  font-size: 13px;
}
table.wholePage td.main-part table td.left-nav a {
  color: #999999;
}
table.wholePage td.main-part table td.left-nav h3 {
  margin: 0;
  padding: 10px 20px;
  font-weight: normal;
  border-bottom: 1px dashed #dfdfdf;
}
table.wholePage td.main-part table td.left-nav ul {
  position: absolute;
  width: 100%;
}
table.wholePage td.main-part table td.left-nav ul li {
  height: 40px;
  width: 100%;
}
table.wholePage td.main-part table td.left-nav ul .menuContent {
  margin: 0,auto;
  width: 100%;
  height: 100%;
}
table.wholePage td.main-part table td.left-nav ul .menuContent i,
table.wholePage td.main-part table td.left-nav ul .menuContent span {
  margin-left: 20px;
  margin-top: 10px;
}
table.wholePage td.main-part table td.left-nav ul li.active,
table.wholePage td.main-part table td.left-nav ul a:hover {
  color: #999999;
  cursor: pointer;
}
table.wholePage td.main-part table td.left-nav ul li.active .menuContent,
table.wholePage td.main-part table td.left-nav ul a:hover .menuContent {
  background-color: #f5f5f5;
  border: 1px solid #dfdfdf;
  border-right: none;
}
table.wholePage td.main-part table td.right-cotent {
  over-flow: hidden;
  position: relative;
}
table.wholePage td.main-part table td.right-cotent iframe.main-frame {
  height: 100%;
  width: 100%;
  border: none;
}
