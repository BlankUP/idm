.list-toolbar {
  background: #fff;
  margin-top: 20px;
  padding: 10px 15px;
  font-size: 12px;
}
.list-toolbar .sort {
  display: inline-block;
  *display: inline;
  *zoom: 1;
}
.list-toolbar .sort a {
  display: inline-block;
  border: 1px solid #eee;
  padding: 5px 10px;
  background: #f8f8f8;
  font-size: 10px;
}
.list-toolbar .sort a .fonticon {
  color: #ccc;
  height: 14px;
  width: 12px;
}
.list-toolbar .sort a:hover,
.list-toolbar .sort a.active {
  border: 1px solid #e79a52;
  color: #fff;
  background: #e79a52;
}
.list-toolbar .sort a:hover .fonticon,
.list-toolbar .sort a.active .fonticon {
  color: #ffffff;
}
.list-toolbar .sort i.fonticon {
  margin-left: 5px;
  font-size: 12px;
}
.list-toolbar .toolbar-filter {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  margin-left: 20px;
}
.list-toolbar .toolbar-filter .textfield {
  width: 60px;
  font-size: 12px;
  text-align: center;
}
.list-toolbar .toolbar-filter button {
  background: #e79a52;
  border: none;
  color: #fff;
  font-size: 12px;
}
.list-toolbar .toolbar-filter button:hover {
  background: #edb47f;
}
.list-toolbar .toolbar-filter button:active {
  background: #edb47f;
}
.list-toolbar .toolbar-tags {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  margin-left: 10px;
}
.list-toolbar .float-right {
  padding-top: 8px;
}
.list-toolbar .pagination {
  margin: 0px;
  height: 20px;
  display: inline-block;
  *display: inline;
  *zoom: 1;
}
.list-toolbar .pagination a,
.list-toolbar .pagination span {
  height: 18px;
  line-height: 18px;
  display: inline;
  float: left;
  margin-left: 3px;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
  outline: none;
  blr: expression(this.hideFocus = true);
}
.list-toolbar .pagination a {
  padding: 0px 5px;
  border: 1px solid #cccccc;
}
.list-toolbar .pagination a:hover {
  border: 1px solid #fd6d01;
  text-decoration: none;
}
.list-toolbar .pagination a.previousPage {
  width: 18px;
  padding: 0px;
  background: url(../shop/images/common.gif) 0px -180px no-repeat;
}
.list-toolbar .pagination a.nextPage {
  width: 18px;
  padding: 0px;
  background: url(../shop/images/common.gif) 0px -210px no-repeat;
}
.list-toolbar .pagination a.lastPage {
  width: 18px;
  padding: 0px;
  background: url(../shop/images/common.gif) -60px -210px no-repeat;
}
.list-toolbar .pagination a.firstPage {
  width: 18px;
  padding: 0px;
  background: url(../shop/images/common.gif) -60px -180px no-repeat;
}
.list-toolbar .pagination span.previousPage {
  width: 18px;
  cursor: default;
  border: 1px solid #cccccc;
  background: url(../shop/images/common.gif) -30px -180px no-repeat;
}
.list-toolbar .pagination span.nextPage {
  width: 18px;
  cursor: default;
  border: 1px solid #cccccc;
  background: url(../shop/images/common.gif) -30px -210px no-repeat;
}
.list-toolbar .pagination span.lastPage {
  width: 18px;
  cursor: default;
  border: 1px solid #cccccc;
  background: url(../shop/images/common.gif) -90px -210px no-repeat;
}
.list-toolbar .pagination span.firstPage {
  width: 18px;
  cursor: default;
  border: 1px solid #cccccc;
  background: url(../shop/images/common.gif) -90px -180px no-repeat;
}
.list-toolbar .pagination .currentPage {
  padding: 0px 5px;
  color: #fd6d01!important;
  font-weight: bold;
  cursor: default;
  border: 1px solid #fd6d01;
  background-color: #ffede1;
}
.list-toolbar .pagination .pageBreak {
  padding: 0px 5px;
  color: #3366cc;
  cursor: default;
}
.productList {
  /* ---------- ProductList ---------- */
}
.productList .nocontent {
  margin: 20px 0px;
  background: #fff;
  padding: 20px 0px;
}
.productList .nocontent dl dt {
  font-size: 18px;
  margin-left: 20px;
  margin-bottom: 20px;
}
.productList .nocontent dl dd {
  color: #999;
  margin-bottom: 10px;
}
.productList .filter {
  margin-bottom: 4px;
}
.productList .filter .title {
  height: 30px;
  line-height: 30px;
  padding-left: 10px;
  color: #6d6d6d;
  font-size: 14px;
  font-weight: bold;
  font-family: "Microsoft YaHei";
  border: solid 1px #e4e4e4;
}
.productList .filter .content {
  padding: 0px 10px;
  border-left: solid 1px #e4e4e4;
  border-right: solid 1px #e4e4e4;
}
.productList .filter dl {
  width: 678px;
  line-height: 30px;
  position: relative;
  overflow: hidden;
  *zoom: 1;
  border-bottom: dotted 1px #e8e8e8;
}
.productList .filter dl.last {
  border-bottom: none;
}
.productList .filter dt {
  width: 80px;
  float: left;
  margin-right: 8px;
  text-align: right;
  white-space: nowrap;
  overflow: hidden;
  cursor: default;
}
.productList .filter dd {
  float: left;
  margin-right: 12px;
  color: #1859a5;
  white-space: nowrap;
}
.productList .filter dd a {
  line-height: 30px;
  padding: 2px 4px;
  color: #2953a5;
}
.productList .filter dd a.current {
  color: #ffffff;
  background-color: #ff6600;
}
.productList .filter .moreOption {
  width: 12px;
  height: 30px;
  line-height: 30px;
  display: none;
  margin-right: 0px;
  position: absolute;
  top: 0px;
  right: 0px;
  color: #7d7d7d;
  cursor: pointer;
  overflow: hidden;
  background: url(../shop/images/product.gif) -210px -270px no-repeat;
}
.productList .filter dd.close {
  background: url(../shop/images/product.gif) -210px -300px no-repeat;
}
.productList .filter .moreFilter {
  line-height: 6px;
  background: url(../shop/images/product.gif) 0px 0px repeat-x;
}
.productList .filter .moreFilter a {
  width: 108px;
  height: 27px;
  line-height: 22px;
  display: block;
  margin: 0px auto;
  text-align: center;
  outline: none;
  blr: expression(this.hideFocus = true);
  background: url(../shop/images/product.gif) 0px -480px no-repeat;
}
.productList .filter .moreFilter .close {
  background: url(../shop/images/product.gif) -120px -480px no-repeat;
}
