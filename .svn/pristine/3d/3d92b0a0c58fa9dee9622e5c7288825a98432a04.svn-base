.flag,
.badge {
  display: inline-block;
  padding: 2px 4px;
  font-size: 11.844px;
  font-weight: bold;
  line-height: 14px;
  color: #ffffff;
  vertical-align: baseline;
  white-space: nowrap;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #cccccc;
}
.flag {
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
}
.badge {
  padding-left: 9px;
  padding-right: 9px;
  -webkit-border-radius: 9px;
  -moz-border-radius: 9px;
  border-radius: 9px;
}
.flag:empty,
.badge:empty {
  display: none;
}
a.flag:hover,
a.flag:focus,
a.badge:hover,
a.badge:focus {
  color: #ffffff;
  text-decoration: none;
  cursor: pointer;
}
.flag-important,
.badge-important {
  background-color: #b94a48;
}
.flag-important[href],
.badge-important[href] {
  background-color: #953b39;
}
.flag-warning,
.badge-warning {
  background-color: #f89406;
}
.flag-warning[href],
.badge-warning[href] {
  background-color: #c67605;
}
.flag-success,
.badge-success {
  background-color: #468847;
}
.flag-success[href],
.badge-success[href] {
  background-color: #356635;
}
.flag-info,
.badge-info {
  background-color: #ff6600;
}
.flag-info[href],
.badge-info[href] {
  background-color: #cc5200;
}
.flag-inverse,
.badge-inverse {
  background-color: #707070;
}
.flag-inverse[href],
.badge-inverse[href] {
  background-color: #575757;
}
.btn .flag,
.btn .badge {
  position: relative;
  top: -1px;
}
.btn-mini .flag,
.btn-mini .badge {
  top: 0;
}
body,
table {
  margin: 0;
  font-size: 12px;
}
ul {
  margin: 0;
  padding: 0;
  list-style: none;
}
form {
  margin: 0;
}
dl {
  margin: 0;
  padding: 0;
}
button {
  cursor: pointer;
}
h1,
h2,
h3,
h4,
h5 {
  position: relative;
}
